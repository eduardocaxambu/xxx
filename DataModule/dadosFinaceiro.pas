procedure TDataFinanceiro.CARTOESITENSDIASGetText(Sender: TField;
  var Text: string; DisplayText: Boolean);
begin
  if DataFinanceiro.CARTOESITENS.IsEmpty then
    Text:=''
  else begin
    if DATE>DataFinanceiro.CARTOESITENS.FieldByName('VENCIMENTO').AsDateTime then
      Text:= IntToStr(DAYSBETWEEN(DATE,DataFinanceiro.CARTOESITENS.FieldByName('VENCIMENTO').AsDateTime))
    else
      Text:= IntToStr(DAYSBETWEEN(DATE,DataFinanceiro.CARTOESITENS.FieldByName('VENCIMENTO').AsDateTime)*-1);
  end;
end;

